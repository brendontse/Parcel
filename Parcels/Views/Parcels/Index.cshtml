@using Parcels.Models;

<h1>To Do List</h1>

@if (Model.Count == 0)
{
    <p>There are no items in the list.</p>
}
<ul>
    @foreach (Parcel parcel in Model)
    {
        <li>@parcel.Description</li>
        <li>@parcel.Weight</li>
        <li>@parcel.Length</li>
        <li>@parcel.Width</li>
        <li>@parcel.Height</li>
        <li>@parcel.Price</li>
    }
</ul>